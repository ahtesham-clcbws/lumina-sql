
{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["www/src/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["www/**/*.ts", "www/**/*.tsx"],
  "exclude": ["node_modules"]
}
